    "@emotion/styled": "^11.11.0",
    "@lemonsqueezy/lemonsqueezy.js": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/supabase-js": "^2.39.7",
    "flutterwave-node-v3": "^1.1.7",
    "flutterwave-react-v3": "^1.3.2",
    "framer-motion": "^10.10.0",
    "next": "^13.4.2",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
