    "@types/react": "^18.2.6",
    "commitizen": "^4.3.0",
    "commitlint": "^17.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-sznm": "^1.1.0",
    "lint-staged": "^13.1.2",
    "next-sitemap": "^4.0.6",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "turbo": "^1.8.8",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
